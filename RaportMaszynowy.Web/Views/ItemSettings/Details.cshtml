@model RaportManager.Domian.Settings

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Setting</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.isActive)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Content)
        </dd>
    </dl>

    <input type="button" name="ChangeIsActive" value="ChangeIsActive" onclick="location.href='@Url.Action("SetActive", "ItemSettings", new { id = Model.SettingsID })'" />
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.SettingsID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
