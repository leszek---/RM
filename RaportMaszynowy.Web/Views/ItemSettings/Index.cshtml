@model IEnumerable<RaportManager.Domian.Settings>

@{
    ViewBag.Title = "Index";
}

<h2>Charakterystyka Przedmiotu</h2>

<p>
    @Html.ActionLink("Dodaj Nową", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Aktywna
        </th>
        <th>
            Opis
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Content)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SettingsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SettingsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SettingsID})
        </td>
    </tr>
}

</table>
