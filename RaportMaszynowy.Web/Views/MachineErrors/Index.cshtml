@model IEnumerable<RaportManager.Domian.MachineError>

@{
    ViewBag.Title = "Index";
}

<h2>Awarie</h2>

<table class="table">
    <tr>
        <th>
            Data
        </th>
        <th>
            Opis
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MachineErrorDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>

        <td>
            @Html.ActionLink("Edycja", "Edit", new { id=item.MachineErrorID }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.MachineErrorID }) |
        </td>
    </tr>
}

</table>
